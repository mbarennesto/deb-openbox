#
# These things are run when an Openbox X Session is started.
#
# compositor
picom &

#notifications
dunst &

# wallpaper
feh --bg-fill ~/Immagini/wallpaper.png &

# panel, taskbar systray, clock
tint2 &

#keyboard
setxkbmap us intl &

#volume 
(sleep 10; volumeicon) &

#network manager
nm-applet &